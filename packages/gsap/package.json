{
  "name": "@remotion-reloaded/gsap",
  "version": "0.0.1",
  "description": "GSAP timeline synchronization for Remotion",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run"
  },
  "peerDependencies": {
    "gsap": "^3.12.0",
    "react": "^18.0.0 || ^19.0.0",
    "remotion": "^4.0.0"
  },
  "dependencies": {
    "@remotion-reloaded/config": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.13",
    "gsap": "^3.12.0",
    "jsdom": "^28.0.0",
    "react": "^19.0.0",
    "remotion": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "license": "MIT"
}
