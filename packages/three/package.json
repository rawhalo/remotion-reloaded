{
  "name": "@remotion-reloaded/three",
  "version": "0.0.1",
  "description": "WebGPU/Three.js integration for Remotion",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run"
  },
  "peerDependencies": {
    "@react-three/fiber": "^9.0.0",
    "@remotion/three": "^4.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "remotion": "^4.0.0",
    "three": ">=0.160.0"
  },
  "dependencies": {
    "@remotion-reloaded/config": "workspace:*"
  },
  "devDependencies": {
    "@react-three/fiber": "^9.0.0",
    "@remotion/three": "^4.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "@types/three": "^0.182.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-test-renderer": "^19.0.0",
    "remotion": "^4.0.0",
    "three": "^0.172.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "license": "MIT"
}
