{
  "name": "@remotion-reloaded/config",
  "version": "1.0.0",
  "description": "Configuration helpers for Remotion Reloaded",
  "license": "MIT",
  "homepage": "https://github.com/rawhalo/remotion-reloaded#readme",
  "bugs": {
    "url": "https://github.com/rawhalo/remotion-reloaded/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rawhalo/remotion-reloaded.git",
    "directory": "packages/config"
  },
  "keywords": [
    "remotion",
    "video",
    "configuration"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "remotion": "^4.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "remotion": "^4.0.0",
    "react": "^19.0.0",
    "@types/react": "^19.0.0",
    "tsup": "^8.0.0",
    "vitest": "^3.0.0",
    "typescript": "^5.7.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
